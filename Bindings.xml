<Bindings>
    <Binding name="TOGGLE_MOUSELOOK" header="BINDMOUSELOOK_TITLE" category="ADDONS">
        if IsMouselooking() then
            MouselookStop()
        else
            MouselookStart()
        end
    </Binding>
</Bindings> 
